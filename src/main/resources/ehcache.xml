<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
         name="CM1"
         updateCheck="false"
         maxBytesLocalHeap="16M">

    <!--
      This is a default configuration, it is re-configured by the CacheConfiguration Spring Bean, using the
      properties from the resources/config/*.yml files.
    -->

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
            eternal="false"
            overflowToDisk="false"
            />

    <cache name="com.cazen.iti.domain.User"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.cazen.iti.domain.Authority"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.cazen.iti.domain.User.authorities"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.cazen.iti.domain.PersistentToken"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.cazen.iti.domain.User.persistentTokens"
           timeToLiveSeconds="3600">
    </cache>
    

    <cache name="com.cazen.iti.social.SocialUserConnection"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.cazen.iti.domain.CommonCode"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.cazen.iti.domain.QuestionMaster"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.cazen.iti.domain.QuestionMaster.rightAnswers"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.cazen.iti.domain.QuestionMaster.wrongAnswers"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.cazen.iti.domain.RightAnswer"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.cazen.iti.domain.WrongAnswer"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.cazen.iti.domain.QuestionVote"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.cazen.iti.domain.UpQuestionMaster"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.cazen.iti.domain.UpQuestionMaster.uprightAnswers"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.cazen.iti.domain.UpQuestionMaster.upwrongAnswers"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.cazen.iti.domain.UpRightAnswer"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.cazen.iti.domain.UpWrongAnswer"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.cazen.iti.domain.UpQuestionVote"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.cazen.iti.domain.UpQuestionMaster.upRightAnswers"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.cazen.iti.domain.UpQuestionMaster.upWrongAnswers"
        timeToLiveSeconds="3600">
    </cache>

    <!-- jhipster-needle-ehcache-add-entry -->
</ehcache>
