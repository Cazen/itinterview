<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns='http://www.ehcache.org/v3'
         xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.0.xsd">

    <cache-template name="simple">
        <expiry>
            <ttl unit="seconds">3600</ttl>
        </expiry>
        <heap>100</heap>
    </cache-template>

    <cache alias="com.cazen.iti.domain.User" uses-template="simple"/>

    <cache alias="com.cazen.iti.domain.Authority" uses-template="simple"/>

    <cache alias="com.cazen.iti.domain.User.authorities" uses-template="simple"/>

    <cache alias="com.cazen.iti.domain.PersistentToken" uses-template="simple"/>

    <cache alias="com.cazen.iti.domain.User.persistentTokens" uses-template="simple"/>

    <cache alias="com.cazen.iti.social.SocialUserConnection" uses-template="simple"/>

    <cache alias="com.cazen.iti.domain.CommonCode" uses-template="simple"/>

    <cache alias="com.cazen.iti.domain.QuestionVote" uses-template="simple"/>

    <cache alias="com.cazen.iti.domain.UpQuestionMaster" uses-template="simple"/>

    <cache alias="com.cazen.iti.domain.UpQuestionMaster.upRightAnswers" uses-template="simple"/>

    <cache alias="com.cazen.iti.domain.UpQuestionMaster.upWrongAnswers" uses-template="simple"/>

    <cache alias="com.cazen.iti.domain.UpRightAnswer" uses-template="simple"/>

    <cache alias="com.cazen.iti.domain.WrongAnswer" uses-template="simple"/>

    <cache alias="com.cazen.iti.domain.QuestionMaster" uses-template="simple"/>

    <cache alias="com.cazen.iti.domain.QuestionMaster.rightAnswers" uses-template="simple"/>

    <cache alias="com.cazen.iti.domain.QuestionMaster.wrongAnswers" uses-template="simple"/>

    <cache alias="com.cazen.iti.domain.RightAnswer" uses-template="simple"/>

    <cache alias="com.cazen.iti.domain.UpQuestionVote" uses-template="simple"/>

    <cache alias="com.cazen.iti.domain.UpWrongAnswer" uses-template="simple"/>

    <cache alias="com.cazen.iti.domain.QuestionMasterStatics" uses-template="simple"/>

    <!-- jhipster-needle-ehcache-add-entry -->
</config>
